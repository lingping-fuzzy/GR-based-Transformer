{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "MLP",
    "dataset": "Peking_1",
    
    "out_dir": "out/Peking_1_graph_classification/",

    "params": {
        "seed": 41,
        "epochs": 200,
        "batch_size": 20,
        "init_lr": 7e-4,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 25,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 2,
        "hidden_dim": 20,
        "out_dim": 20,
        "readout": "mean",
        "gated": false,
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "layer_norm": false,
		"pos_enc": false,
		 "lap_pos_enc": false,
        "pos_enc_dim": 2
    }
}